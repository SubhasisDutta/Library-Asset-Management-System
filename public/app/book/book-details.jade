.container
  h1 {{book.title}}
  .row
    .col-md-6
      h3 Book ISBN      : {{book.isbn}}
      h3 Book ISBN 13   : {{book.ISBN13}}
      h3 Book Publisher : {{book.Publisher}}
      h3 Book Pages     : {{book.pages}}


    .col-md-6
      .panel.panel-primary
        .panel-heading Cover Page
        .panel-body
          img(src="{{book.cover}}",style="height:200px;")